{{#if category}}
<h1 class="col-md-6 col-md-offset-3 poll-head text-center">{{title}}</h1>
{{#each category}}
<div class="row">
    <div class="text-center category-wrap col-md-4 col-md-offset-4">
        <a href="/polls/show/{{this.category}}">{{this.category}}</a>
    </div>
</div>
{{/each}}
{{else}}
<h1 class="category-wrap text-center">There are no Categories available!</h1>
{{/if}}
<p class="category-wrap text-center col-md-6 col-md-offset-3"><a href="/dashboard">Return to Dashboard</a></p>